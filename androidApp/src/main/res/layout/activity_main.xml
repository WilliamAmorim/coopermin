            android:layout_height="120dp"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:layout_marginRight="10dp"
            android:scaleType="fitXY"
            android:src="@drawable/rectangle_gray_box_home"
            app:riv_border_color="@color/transparent"
            app:riv_border_width="0dip"
            app:riv_corner_radius="10dp"
            app:riv_mutate_background="true"
            app:riv_oval="false"
            app:riv_tile_mode="clamp" />

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="11dp"
            android:layout_marginTop="7dp"
            android:fontFamily="@font/sf_pro_display_semibold"
            android:text="1kg"
            android:textColor="@color/green"
            android:textSize="12sp" />

        <TextView
            android:id="@+id/txt_Title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginLeft="11dp"
            android:layout_marginRight="11dp"
            android:layout_marginTop="7dp"
            android:fontFamily="@font/sf_pro_display_semibold"
            android:text="coriander product ntitle will  be here"
            android:textColor="@color/txt_black"
            android:textSize="12sp" />

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="11sp"
            android:layout_marginRight="9dp"
            android:orientation="horizontal">

          <FrameLayout
              android:layout_width="wrap_content"
              android:layout_height="wrap_content">

            <TextView
                android:id="@+id/txt_addItem"
                android:layout_width="match_parent"
                android:layout_height="25dp"
                android:gravity="center"
                android:background="@drawable/rectangle_green_btn"
                android:fontFamily="@font/sf_pro_display_semibold"
                android:paddingLeft="24dp"
                android:paddingRight="24dp"
                android:text="add"
                android:visibility="gone"
                android:textColor="@color/white"
                android:textSize="11sp" />

            <LinearLayout
                android:id="@+id/li_cart"
                android:layout_width="wrap_content"
                android:layout_height="25dp"
                android:visibility="gone"
                android:background="@drawable/rectangle_green_btn"
                android:orientation="horizontal">


              <LinearLayout
                  android:id="@+id/li_sub"
                  android:layout_width="25dp"
                  android:layout_height="25dp"
                  android:background="@drawable/circle_green">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/ic_sub" />
              </LinearLayout>
              <TextView
                  android:id="@+id/txt_count"
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:text="1"
                  android:textSize="12sp"
                  android:textColor="@color/white"
                  android:fontFamily="@font/sf_pro_display_bold"
                  android:layout_gravity="center"
                  android:paddingLeft="6dp"
                  android:paddingRight="6dp"/>
              <LinearLayout
                  android:id="@+id/li_add"
                  android:layout_width="25dp"
                  android:layout_height="25dp"
                  android:background="@drawable/circle_green">

                <ImageView
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:src="@drawable/ic_add" />
              </LinearLayout>
            </LinearLayout>
          </FrameLayout>

          <TextView
              android:id="@+id/txt_price"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="center"
              android:fontFamily="@font/sf_pro_display_semibold"
              android:text="R$120"
              android:textColor="@color/green"
              android:textSize="14sp" />

          <TextView
              android:layout_width="0dp"
              android:layout_height="wrap_content"
              android:layout_gravity="center"
              android:layout_marginLeft="5dp"
              android:layout_weight="1"
              android:fontFamily="@font/sf_pro_display_regular"
              android:text="R$150"
              android:textAlignment="textStart"
              android:textColor="@color/subtxt_gray"
              android:textSize="12sp" />


        </LinearLayout>
      </LinearLayout>
